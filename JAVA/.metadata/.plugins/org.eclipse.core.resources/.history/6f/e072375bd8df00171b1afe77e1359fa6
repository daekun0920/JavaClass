
public class Ex34_static {

	public static void main(String[] args) {
		
		// Ex34_static.java
		
		// static 키워드
		//   - 정적 키워드
		//   - 클래스 멤버에 붙이는 키워드
		// 1. 멤버 변수
		// 2. 멤버 메소드
		
		Student s1 = new Student();
		s1.setName("홍길동");
		s1.setAge(15);
		s1.setSchool("역삼 중학교");
		
		Student s2 = new Student();
		s2.setName("아무개");
		s2.setAge(14);
		s2.setSchool("역삼 중학교");
		
		Student s3 = new Student();
		s3.setName("하하하");
		s3.setAge(14);
		s3.setSchool("역삼 중학교");
		
		
	}

}

// 학생 클래스
//   - "역삼 중학교"
class Student {
	
	private String name;
	private int age;
	private String school;
	
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	public int getAge() {
		return age;
	}
	public void setAge(int age) {
		this.age = age;
	}
	public String getSchool() {
		return school;
	}
	public void setSchool(String school) {
		this.school = school;
	}
	
	
	
}












